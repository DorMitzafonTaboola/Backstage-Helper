{
  "manifest_version": 2,

  "name": "Taboola's Backstage helper",
  "description": "Highlights important buttons and details on Backstage",
  "version": "1.0",
  "icons": {
    "16": "Marker.png",
    "48": "Marker.png",
    "128": "Marker.png"
  },

  "browser_action": {
    "default_title": "BackStage Helper"
  },

  "background": {
    "scripts": ["jquery-3.2.1.min.js","background.js"]
  },
  "permissions": [
    "background",
    "activeTab",
    "tabs",
    "https://backstage.taboola.com/*"
  ]
}